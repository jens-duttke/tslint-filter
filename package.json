{
	"name": "tslint-filter",
	"version": "1.7.0",
	"description": "Suppress and modify TSLint linting errors, before they get returned to the console or your code editor",
	"author": "Jens Duttke <github@duttke.de>",
	"keywords": [
		"TSLint",
		"warnings",
		"errors",
		"filter",
		"suppress",
		"hide",
		"ignore",
		"modify",
		"manipulate",
		"rules",
		"rule",
		"proxy"
	],
	"main": "./dist/index.js",
	"license": "MIT",
	"scripts": {
		"dev": "rimraf dist && tsc -w",
		"publish:major": "npm version major",
		"publish:minor": "npm version minor",
		"publish:patch": "npm version patch",
		"preversion": "npm run lint && npm run build && npm run test && assert-outdated-npm-modules --max-warnings 0",
		"postversion": "git push && git push --tags && npm publish",
		"build": "rimraf dist && tsc",
		"lint": "tslint --project tsconfig.json --format verbose",
		"test": "tslint --test test"
	},
	"dependencies": {
		"to-regex-range": "^4.0.3"
	},
	"devDependencies": {
		"@types/node": "^10.12.19",
		"assert-outdated": "^3.2.0",
		"glob": "^7.1.3",
		"rimraf": "^2.6.3",
		"tslint": "^5.12.1",
		"tslint-microsoft-contrib": "^6.0.0",
		"typescript": "^3.2.4"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/jens-duttke/tslint-filter.git"
	},
	"bugs": {
		"url": "https://github.com/jens-duttke/tslint-filter/issues"
	},
	"engines": {
		"node": ">=6.0.0"
	},
	"engineStrict": true
}
